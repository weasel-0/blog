---
import { getCollection } from "astro:content";

export async function getStaticPaths() {
    const pins = await getCollection("pins");
    return pins.map((pin) => ({
        params: { slug: pin.slug },
        props: pin,
    }));
}

const pin = Astro.props;
const { Content } = await pin.render();
